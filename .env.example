# BeraBundle Environment Variables
# Copy this file to .env for local development

# API Key for OogaBooga API
BERABUNDLE_API_KEY=your_api_key_here

# Private key for testing (DO NOT USE IN PRODUCTION)
# BERABUNDLE_PRIVATE_KEY=your_private_key_here

# Environment (development, production)
NODE_ENV=development

# Server flag, forces use of environment variables even in development
# BERABUNDLE_SERVER=true

# Custom API URL (default: https://mainnet.api.oogabooga.io)
# BERABUNDLE_API_URL=https://mainnet.api.oogabooga.io